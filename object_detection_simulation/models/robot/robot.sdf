                        <fy>421.61578369140625</fy>
                        <cx>422.2854309082031</cx>
                        <cy>236.57237243652344</cy>
                        <s>0</s>
                        </intrinsics>
                    </lens>
                </camera>
            </sensor>
        </link>

        <joint name="camera_depth_joint" type="fixed">
            <child>camera_depth_frame</child>
            <parent>camera_front</parent>
